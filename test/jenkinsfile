// This shows a simple example of how to archive the build output artifacts.
node {
    checkout scm
    sh "ls -la ${pwd()}"
    String parentDir = pwd()
    def steps = load "${parentDir}/jenkinsfile"
    def params = [
        "dir" : parentDir
    ]
    stage("demo Stage") {
        step("Step 1") {
            steps.createBuildOutput(params)
        }
        step("Step 2") {
            steps.archiveBuildOutput(params)
        }

    }




}